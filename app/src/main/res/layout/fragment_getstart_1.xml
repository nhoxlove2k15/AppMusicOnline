<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back_ground_get_start">

    <ImageView
        android:id="@+id/imgLogoNit_2"
        android:layout_width="100dp"
        android:layout_height="375dp"
        android:layout_marginTop="44dp"
        android:scaleType="fitXY"
        android:src="@drawable/logo_nit_2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/imgGirl"
        android:layout_width="162dp"
        android:layout_height="413dp"
        android:layout_marginTop="136dp"
        android:layout_marginEnd="100dp"
        android:src="@drawable/girl"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imgBar2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="124dp"
        android:layout_marginBottom="12dp"
        android:src="@drawable/bar_2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imgMusic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:src="@drawable/music"
        app:layout_constraintBottom_toTopOf="@+id/imgGirl"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imgMusic1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/music_1"
        app:layout_constraintBottom_toTopOf="@+id/imgGirl"
        app:layout_constraintEnd_toStartOf="@+id/imgMusic" />

    <ImageView
        android:id="@+id/imgMusic2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="64dp"
        android:src="@drawable/music_2"
        app:layout_constraintBottom_toTopOf="@+id/imgGirl"
        app:layout_constraintEnd_toStartOf="@+id/imgMusic1" />


</androidx.constraintlayout.widget.ConstraintLayout>